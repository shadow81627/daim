  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "now-build": "npm run generate",
    "start": "cross-env NODE_ENV=production nuxt start",
    "start:test": "cross-env NODE_ENV=production PORT=3001 nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . && npm run lint:md",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:md": "remark . -r .remarkrc.js -f",
    "mdspell": "mdspell -r -n -a --en-us content/**/**.md || true",
    "write-good": "write-good content",
    "cspell": "cspell content/**",
    "lhci:autorun": "lhci autorun",
    "images": "node images.js",
    "test": "codeceptjs run --steps",
    "test:e2e": "codeceptjs run --steps",
    "update:snapshots": "rsync -a ./test/e2e/output/ ./test/e2e/screenshots/base"
  },
  "engines": {
    "node": "<=14.x"
  },
