{
  "name": "daim",
  "version": "1.2.2",
  "description": "My extraordinary Nuxt.js project",
  "author": "Damien Robinson",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "generate:modern": "nuxt generate --modern",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lhci:autorun": "lhci autorun",
    "test": "ava",
    "test:unit": "cross-env TEST=unit ava --config unit.config.js",
    "test:e2e": "cross-env TEST=e2e ava --config e2e.config.js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "5.8.0",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/pwa": "3.0.0-beta.19",
    "bootstrap": "4.4.1",
    "bootstrap-vue": "2.1.0",
    "cross-env": "6.0.3",
    "koa": "2.11.0",
    "nuxt": "2.10.2",
    "nuxt-i18n": "6.4.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "1.3.0",
    "@lhci/cli": "0.3.4",
    "@mdi/js": "4.6.95",
    "@nuxtjs/eslint-config": "2.0.0",
    "@nuxtjs/eslint-module": "1.1.0",
    "@nuxtjs/stylelint-module": "3.1.0",
    "@nuxtjs/vuetify": "1.9.0",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "7.0.18",
    "@vue/test-utils": "1.0.0-beta.30",
    "ava": "2.4.0",
    "babel-eslint": "10.0.3",
    "babel-plugin-module-resolver": "3.2.0",
    "browser-env": "3.3.0",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-nuxt": "0.5.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.1.0",
    "lint-staged": "8.2.1",
    "nodemon": "2.0.1",
    "prettier": "1.19.1",
    "require-extension-hooks": "0.3.3",
    "require-extension-hooks-babel": "1.0.0",
    "require-extension-hooks-vue": "2.0.0",
    "semantic-release": "15.13.31",
    "stylelint": "10.1.0"
  }
}
