{
  "name": "daim",
  "version": "1.20.4",
  "description": "Damien Robinson Web Development",
  "author": {
    "name": "Damien Robinson",
    "email": "damien.robinson@daim.dev",
    "url": "https://daim.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shadow81627/daim.git"
  },
  "bugs": "https://github.com/shadow81627/daim/issues",
  "homepage": "https://daim.dev",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build --modern",
    "build:analyze": "nuxt build --analyze",
    "build:export": "npm run build && npm run generate",
    "now-build": "npm run build:export",
    "start": "cross-env NODE_ENV=production nuxt start",
    "serve:lhci": "npm run start",
    "storyblok:export": "storyblok-component-export && npm run lint:fix",
    "storyblok:migrate": "storyblok-migrate --content-migrations --component-migrations",
    "generate": "cross-env HOST=localhost PORT=3001 nuxt export --modern",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . && npm run lint:md",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix && npm run lint:md -- -o",
    "lint:md": "remark . -r .remarkrc.js -f",
    "lhci:autorun": "lhci autorun",
    "images": "node images.js",
    "test": "ava",
    "test:unit": "cross-env TEST=unit ava --config unit.config.js",
    "test:e2e": "cross-env TEST=e2e ava --config e2e.config.js"
  },
  "engines": {
    "node": "12.x"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.30",
    "@fortawesome/free-brands-svg-icons": "5.14.0",
    "@fortawesome/free-regular-svg-icons": "5.14.0",
    "@fortawesome/free-solid-svg-icons": "5.14.0",
    "@fortawesome/vue-fontawesome": "2.0.0",
    "@nuxt/content": "1.8.1",
    "@nuxtjs/axios": "5.12.2",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/markdownit": "1.2.10",
    "@nuxtjs/pwa": "3.0.2",
    "@nuxtjs/sitemap": "2.4.0",
    "cross-env": "7.0.2",
    "github-markdown-css": "4.0.0",
    "koa": "2.13.0",
    "luxon": "1.25.0",
    "mailgo": "0.9.16",
    "nuxt": "2.14.4",
    "nuxt-fontawesome": "0.4.0",
    "nuxt-i18n": "6.13.12",
    "nuxt-webfontloader": "1.1.0"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "1.3.0",
    "@lhci/cli": "0.5.0",
    "@mdi/js": "5.5.55",
    "@nuxtjs/eslint-config": "3.1.0",
    "@nuxtjs/eslint-module": "2.0.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/vuetify": "1.11.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@vue/test-utils": "1.0.5",
    "ava": "3.12.1",
    "babel-eslint": "10.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "browser-env": "3.3.0",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-pngquant": "9.0.0",
    "imagemin-svgo": "8.0.0",
    "lint-staged": "10.3.0",
    "lqip-loader": "2.2.0",
    "nodemon": "2.0.4",
    "nuxt-purgecss": "1.0.0",
    "prettier": "2.1.1",
    "remark-cli": "8.0.1",
    "remark-lint": "7.0.1",
    "remark-preset-lint-consistent": "3.0.1",
    "remark-preset-lint-recommended": "4.0.1",
    "require-extension-hooks": "0.3.3",
    "require-extension-hooks-babel": "1.0.0",
    "require-extension-hooks-vue": "3.0.0",
    "responsive-loader": "2.1.0",
    "semantic-release": "17.1.1",
    "sharp": "0.26.0",
    "sqip-loader": "1.0.0",
    "storyblok-migrate": "0.3.4",
    "stylelint": "13.7.0",
    "webp-loader": "0.6.0"
  }
}
