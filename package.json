{
  "name": "daim",
  "version": "1.18.17",
  "description": "Innovative Marketing Technology Automation",
  "author": "Damien Robinson",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "build:modern": "nuxt build --modern",
    "now-build": "nuxt generate --modern",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "serve:lhci": "npm run start",
    "storyblok:export": "storyblok-component-export && npm run lint:fix",
    "storyblok:migrate": "storyblok-migrate --content-migrations --component-migrations",
    "generate": "nuxt generate",
    "generate:modern": "nuxt generate --modern",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . && npm run lint:md",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix && npm run lint:md -- -o",
    "lint:md": "remark . -r .remarkrc.js -f",
    "lhci:autorun": "lhci autorun",
    "test": "ava",
    "test:unit": "cross-env TEST=unit ava --config unit.config.js",
    "test:e2e": "cross-env TEST=e2e ava --config e2e.config.js"
  },
  "engines": {
    "node": "12.x"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/free-brands-svg-icons": "5.12.1",
    "@fortawesome/free-solid-svg-icons": "5.12.1",
    "@fortawesome/vue-fontawesome": "0.1.9",
    "@nuxtjs/axios": "5.9.5",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/markdownit": "1.2.9",
    "@nuxtjs/pwa": "3.0.0-beta.20",
    "@nuxtjs/sitemap": "2.0.1",
    "bootstrap": "4.4.1",
    "bootstrap-vue": "2.5.0",
    "bootswatch": "4.4.1",
    "cloudinary-core": "2.8.1",
    "cross-env": "7.0.0",
    "github-markdown-css": "4.0.0",
    "koa": "2.11.0",
    "nuxt": "2.11.0",
    "nuxt-fontawesome": "0.4.0",
    "nuxt-i18n": "6.6.0",
    "nuxt-webfontloader": "1.1.0",
    "storyblok-nuxt": "1.1.0"
  },
  "devDependencies": {
    "@bazzite/nuxt-optimized-images": "0.3.0",
    "@lhci/cli": "0.3.9",
    "@mdi/js": "4.9.95",
    "@nuxtjs/eslint-config": "2.0.2",
    "@nuxtjs/eslint-module": "1.1.0",
    "@nuxtjs/stylelint-module": "3.2.2",
    "@nuxtjs/vuetify": "1.11.0",
    "@semantic-release/changelog": "5.0.0",
    "@semantic-release/git": "9.0.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "ava": "3.4.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "browser-env": "3.3.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-nuxt": "0.5.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "imagemin-gifsicle": "6.0.1",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngquant": "8.0.0",
    "imagemin-svgo": "7.1.0",
    "jimp": "0.9.3",
    "lint-staged": "10.0.8",
    "lqip-loader": "2.2.0",
    "nodemon": "2.0.2",
    "nuxt-purgecss": "0.2.1",
    "prettier": "1.19.1",
    "remark-cli": "7.0.1",
    "remark-lint": "6.0.5",
    "remark-preset-lint-consistent": "2.0.3",
    "remark-preset-lint-recommended": "3.0.3",
    "require-extension-hooks": "0.3.3",
    "require-extension-hooks-babel": "1.0.0",
    "require-extension-hooks-vue": "3.0.0",
    "responsive-loader": "1.2.0",
    "semantic-release": "17.0.4",
    "sqip-loader": "1.0.0",
    "storyblok-migrate": "0.3.3",
    "stylelint": "13.2.0",
    "webp-loader": "0.6.0"
  }
}
