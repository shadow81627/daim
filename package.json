{
  "name": "daim",
  "version": "1.12.8",
  "description": "Innovative Marketing Technology Automation",
  "author": "Damien Robinson",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build --modern && nuxt generate --modern && mv dist public",
    "build:analyze": "nuxt build --analyze",
    "build:modern": "nuxt build --modern",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "serve:lhci": "npm run start",
    "storyblok:export": "storyblok-component-export",
    "storyblok:migrate": "storyblok-migrate --content-migrations --component-migrations",
    "generate": "nuxt generate",
    "generate:modern": "nuxt generate --modern",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lhci:autorun": "lhci autorun",
    "test": "ava",
    "test:unit": "cross-env TEST=unit ava --config unit.config.js",
    "test:e2e": "cross-env TEST=e2e ava --config e2e.config.js"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.26",
    "@fortawesome/free-brands-svg-icons": "5.12.0",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "@fortawesome/vue-fontawesome": "0.1.9",
    "@nuxtjs/axios": "5.9.0",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/markdownit": "1.2.7",
    "@nuxtjs/pwa": "3.0.0-beta.19",
    "bootstrap": "4.4.1",
    "bootstrap-vue": "2.1.0",
    "bootswatch": "4.4.1",
    "cloudinary-core": "2.8.0",
    "cross-env": "6.0.3",
    "github-markdown-css": "3.0.1",
    "koa": "2.11.0",
    "nuxt": "2.11.0",
    "nuxt-fontawesome": "0.4.0",
    "nuxt-i18n": "6.4.1",
    "nuxt-webfontloader": "1.1.0",
    "storyblok-nuxt": "1.0.2"
  },
  "devDependencies": {
    "@bazzite/nuxt-optimized-images": "0.3.0",
    "@lhci/cli": "0.3.7",
    "@mdi/js": "4.7.95",
    "@nuxtjs/eslint-config": "2.0.0",
    "@nuxtjs/eslint-module": "1.1.0",
    "@nuxtjs/stylelint-module": "3.1.1",
    "@nuxtjs/vuetify": "1.9.1",
    "@semantic-release/changelog": "3.0.6",
    "@semantic-release/git": "7.0.18",
    "@vue/test-utils": "1.0.0-beta.30",
    "ava": "2.4.0",
    "babel-eslint": "10.0.3",
    "babel-plugin-module-resolver": "4.0.0",
    "browser-env": "3.3.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-nuxt": "0.5.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "3.1.0",
    "imagemin-gifsicle": "6.0.1",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngquant": "8.0.0",
    "imagemin-svgo": "7.0.0",
    "jimp": "0.9.3",
    "lint-staged": "9.5.0",
    "lqip-loader": "2.2.0",
    "nodemon": "2.0.2",
    "nuxt-purgecss": "0.2.1",
    "prettier": "1.19.1",
    "require-extension-hooks": "0.3.3",
    "require-extension-hooks-babel": "1.0.0",
    "require-extension-hooks-vue": "3.0.0",
    "responsive-loader": "1.2.0",
    "semantic-release": "15.14.0",
    "sqip-loader": "1.0.0",
    "storyblok-migrate": "0.3.3",
    "stylelint": "12.0.0",
    "webp-loader": "0.6.0"
  }
}
