{
  "ci": {
    "collect": {
      "numberOfRuns": 5,
      "url": [
        "http://localhost:3000/",
        "http://localhost:3000/resume"
      ]
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "color-contrast": "off",
        "unused-css-rules": "off",
        "uses-http2": "off",
        "unused-javascript": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
